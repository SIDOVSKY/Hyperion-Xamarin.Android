<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/01/nuspec.xsd">
  <metadata>
    <id>Hyperion-Xamarin.Android</id>
    <version>1.0.0</version>
    <title>Hyperion-Xamarin.Android</title>
    <authors>SIDOVSKY</authors>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/SIDOVSKY/Hyperion-Xamarin.Android</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Hyperion module pack for Xamarin.Android apps</description>
    <copyright>Vadim Sedov</copyright>
	<icon>icon.png</icon>
	<iconUrl>https://raw.githubusercontent.com/SIDOVSKY/Hyperion-Xamarin.Android/master/art/icon.png</iconUrl>
    <tags>xamarin android monodroid xamarin.android hyperion</tags>
    <dependencies>
      <group targetFramework="MonoAndroid">
        <dependency id="Xamarin.Android.Support.Design" version="28.0.0.3" />
        <dependency id="Xamarin.Google.Dagger" version="2.25.2.1" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="art\icon.png" target="" />
    <file src="**\*.props" target="build" />
    <file src="HyperionAndroidLibs\**" target="HyperionAndroidLibs" />
  </files>
</package>